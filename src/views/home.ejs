<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="http://localhost:8000/css/style.css">
    <title>Document</title>
</head>

<body>
    <h1>List Users</h1>
    <table>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>City</th>
        </tr>

        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id  %> </td>
                <td><%= user.name  %> </td>
                <td><%= user.email  %> </td>
                <td><%= user.city  %> </td>
            </tr>
        <% }) %>
        
    </table>

</body>

</html>